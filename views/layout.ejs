<!DOCTYPE html>
<html lang="en">
  <%- include("./includes/head.ejs") %>
  <body>
    <%- include("./includes/navbar.ejs") %> <%- include("./includes/theme.ejs")
    %>
    <div class="h-100 container py-2">
      <h1 class="fw-light">Dashboard</h1>
      <%- include("./includes/breadcrumb.ejs") %> <% if (messages.info) { %>
      <div class="alert alert-primary" role="alert"><%= messages.info %></div>
      <% } %> <% if (messages.error) { %>
      <div class="alert alert-danger" role="alert"><%= messages.error %></div>
      <% } %> <% if (messages.success) { %>
      <div class="alert alert-success" role="alert">
        <%= messages.success %>
      </div>
      <% } %> <%- body -%>
    </div>
    <%- include("./includes/foot.ejs") %>
  </body>
</html>
